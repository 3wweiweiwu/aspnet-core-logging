<?xml version="1.0" encoding="utf-8" ?>
<SonarQubeAnalysisProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                             xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
                             xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">

  <!--
    Analysis parameters are defined here: https://docs.sonarqube.org/latest/analysis/analysis-parameters/.
  -->
  <Property Name="sonar.host.url">https://sonarcloud.io</Property>
  <Property Name="sonar.organization">satrapu-github</Property>
  <Property Name="sonar.projectKey">aspnet-core-logging</Property>
  <!--
  <Property Name="sonar.projectVersion">1.0.0-SNAPSHOT</Property>
  -->
  <Property Name="sonar.sourceEncoding">UTF-8</Property>
  <Property Name="sonar.verbose">true</Property>
  <!--
  <Property Name="sonar.tests">Tests/**</Property>
  <Property Name="sonar.test.exclusions">Tests/**</Property>
  -->

  <!--
    Specify the location of code coverage data files in OpenCover format.
    See more here: https://docs.sonarqube.org/pages/viewpage.action?pageId=6389770.
  -->
  <Property Name="sonar.cs.opencover.reportsPaths">Tests/UnitTests/**/*.opencover.xml</Property>
  <Property Name="sonar.cs.opencover.it.reportsPaths">Tests/IntegrationTests/**/*.opencover.xml</Property>

</SonarQubeAnalysisProperties>